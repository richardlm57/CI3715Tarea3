class ParkingLot():
    
    def __init__(self):
        self._reservation = []
        self._maxspots = 10
    
    
    def addReservation(self, start, end):
        
        assert(start < end)
        assert (start >= 6 and start < 18)
        assert (end > 6 and end <= 18)
        
        
        
    def marzulloA(r):

        n = len(r)/2
        return [n, r[0][0],r[1][0]]

