import unittest
from marzullo import *

class testMarzullo(unittest.TestCase):
    
    def testMarzulloExists(self):
        marzulloAlg([[6,-1],[7,1]])
        
    def testOneInterval(self):
        self.assertEqual(marzulloA([[6,-1],[7,1]]),[1,6,7])
        
    